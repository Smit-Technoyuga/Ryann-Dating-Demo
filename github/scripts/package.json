name: ryann_dating
description: "Ryann Dating App"
publish_to: "none" # Remove this line if you wish to publish to pub.dev

version: 1.0.0+1

environment:
  sdk: ^3.10.1

dependencies:
  flutter:
    sdk: flutter
  flutter_localizations:
    sdk: flutter

  cupertino_icons: ^1.0.8
  go_router: ^17.0.1
  cached_network_image: ^3.4.1
  flutter_svg: ^2.2.3
  flutter_screenutil: ^5.9.3
  flutter_bloc: ^9.1.1
  equatable: ^2.0.8
  fluttertoast: ^9.0.0
  pinput: ^6.0.1
  intl: ^0.20.2
  image_picker: ^1.1.2
  permission_handler: ^11.3.1
  retrofit: ^4.9.2
  injectable: ^2.7.1+4
  get_it: ^9.2.0
  pretty_dio_logger: ^1.4.0
  dio: ^5.9.1
  json_annotation: ^4.10.0
  flutter_easyloading: ^3.0.5
  shimmer_animation: ^2.2.2+1
  pull_to_refresh: ^2.0.0
  connectivity_plus: ^7.0.0
  shared_preferences: ^2.5.2
  flutter_liveness_check: ^1.0.15
  camera: ^0.10.6

dev_dependencies:
  flutter_test:
    sdk: flutter

  flutter_lints: ^6.0.0
  flutter_gen_runner: ^5.12.0
  build_runner: ^2.10.5
  retrofit_generator: ^10.2.1
  injectable_generator: ^2.12.0
  json_serializable: ^6.12.0

flutter:
  uses-material-design: true
  generate: true

  assets:
    - assets/logo/
    - assets/svg_icons/
    - assets/images/
    - assets/svg_icons/dashboard_icons/

  fonts:
    - family: Poppins
      fonts:
        - asset: assets/fonts/Poppins-Regular.ttf
        - asset: assets/fonts/Poppins-Bold.ttf
        - asset: assets/fonts/Poppins-Light.ttf
        - asset: assets/fonts/Poppins-Medium.ttf
        - asset: assets/fonts/Poppins-SemiBold.ttf
        - asset: assets/fonts/Poppins-Black.ttf
